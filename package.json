{
  "name": "flash_card_app",
  "version": "1.0.0",
  "description": "A flash card app.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "front-install": "cd frontend && npm install && cd ..",
    "frontend": "npm start --prefix frontend",
    "server:dev": "concurrently \"npm run server:debug\" \"npm run frontend \""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "nodemon": "^1.19.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pm2": "^5.2.0",
    "sequelize": "^5.19.0",
    "sequelize-cli": "^5.5.1",
    "validator": "^10.9.0"
  },
  "author": "Jian Wu",
  "license": "MIT"
}
